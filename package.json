{
  "name": "some-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env mode=development",
    "build": "webpack build --env mode=production",
    "test:unit": "npx jest --config ./config/jest/jest.config.ts",
    "test:screenshot": "npx loki test",
    "test:screenshot:ok": "npx loki approve",
    "test:screenshot:visual-diff": "npm run test:screenshot:json-diff && npm run test:screenshot:html-diff",
    "test:screenshot:json-diff": "npx ts-node scripts/generate-visual-report-json.ts",
    "test:screenshot:html-diff": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:screenshot:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "npx eslint \"**/*.{ts,tsx}\"",
    "eslint:fix": "npx eslint '**/*.{ts,tsx}' --fix",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
    "storybook": "storybook dev -p 6006 -c ./config/storybook ",
    "storybook:build": "storybook build -c ./config/storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
